{
    "name": "react-imageViewer",
    "version": "1.0.0",
    "description": "react image viewer",
    "main": "index.js",
    "author": "jinxin",
    "license": "MIT",
    "engines": {
        "node": ">=6.5.0",
        "npm": "^4.0.0"
    },
    "scripts": {
        "clean": "rimraf dist",
        "compile": "better-npm-run compile",
        "lint": "eslint bin build config server src tests",
        "lint:fix": "npm run lint -- --fix",
        "start": "better-npm-run start",
        "dev": "better-npm-run dev",
        "test": "better-npm-run test",
        "test:dev": "npm run test -- --watch",
        "deploy": "better-npm-run deploy",
        "deploy:dev": "better-npm-run deploy:dev",
        "deploy:prod": "better-npm-run deploy:prod",
        "codecov": "cat coverage/*/lcov.info | codecov"
    },
    "betterScripts": {
        "compile": {
            "command": "node bin/compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "dev": {
            "command": "nodemon --exec node bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy": {
            "command": "npm run clean && npm run compile",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "deploy:dev": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "deploy:prod": {
            "command": "npm run deploy",
            "env": {
                "NODE_ENV": "production",
                "DEBUG": "app:*"
            }
        },
        "start": {
            "command": "node bin/server",
            "env": {
                "DEBUG": "app:*"
            }
        },
        "test": {
            "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
            "env": {
                "NODE_ENV": "test",
                "DEBUG": "app:*"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "jinxin"
    },
    "dependencies": {
        "bce-sdk-js": "^0.2.3",
        "classnames": "^2.2.5",
        "css-loader": "^0.28.0",
        "cssnano": "^3.10.0",
        "debug": "^2.6.3",
        "exports-loader": "^0.6.4",
        "fetch-cookie": "^0.6.0",
        "file-loader": "^0.9.0",
        "fs-extra": "^0.30.0",
        "html-webpack-plugin": "^2.28.0",
        "imports-loader": "^0.6.5",
        "install": "^0.8.8",
        "ip": "^1.1.2",
        "json-loader": "^0.5.4",
        "lodash": "^4.16.1",
        "node-sass": "^3.7.0",
        "postcss-flexibility": "^1.1.0",
        "postcss-loader": "^1.3.0",
        "postcss-modules-values": "^1.2.2",
        "prop-types": "^15.5.8",
        "qrcode.react": "^0.7.1",
        "rc-tabs": "^7.5.0",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-hot-loader": "^3.0.0-beta.6",
        "react-tabs": "^1.0.0",
        "react-transition-group": "^1.1.3",
        "rimraf": "^2.5.4",
        "style-loader": "^0.13.1",
        "webpack": "^2.5.1",
        "whatwg-fetch": "^2.0.3",
        "yargs": "^7.1.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.5.3",
        "babel-core": "^6.23.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.3.1",
        "babel-plugin-async-to-promises": "^1.0.5",
        "babel-plugin-import": "^1.1.1",
        "babel-plugin-istanbul": "^2.0.1",
        "babel-plugin-transform-class-properties": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-polyfill": "^6.22.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.22.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.22.0",
        "babel-register": "^6.14.0",
        "babel-runtime": "^6.11.6",
        "better-npm-run": "^0.0.14",
        "connect-history-api-fallback": "^1.3.0",
        "eslint": "^3.15.0",
        "eslint-config-standard": "^6.0.0",
        "eslint-config-standard-react": "^4.0.0",
        "eslint-loader": "^1.4.1",
        "eslint-plugin-babel": "^4.0.1",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^6.2.2",
        "eslint-plugin-standard": "^2.0.1",
        "express": "^4.14.0",
        "extract-text-webpack-plugin": "^2.0.0-rc.3",
        "nodemon": "^1.10.2",
        "react-addons-perf": "^15.4.2",
        "react-scripts": "^0.6.0",
        "redbox-react": "^1.2.10",
        "url-loader": "^0.5.7",
        "webpack-dev-middleware": "^1.10.0",
        "webpack-hot-middleware": "^2.17.0"
    }
}
